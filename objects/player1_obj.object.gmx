            <kind>1</kind>
            <string>///Controls
//Xbox controls
var dir1 = point_direction(0, 0, xaxis, yaxis);
var dir2 = round(point_direction(x,y, player1_obj.x,player1_obj.y)/90)*90
scr_get_gampad_input(0);

scr_move_axis(xaxis, yaxis, 0.25, 16);


if ((gamepad_button_check_pressed(0, gp_face1)) == true &amp;&amp; mp_current &gt;= 20){
    action_create_object_motion(spell1_obj, player1_obj.x+(xaxis*20), player1_obj.y+(yaxis*20), 35, dir1);
    mp_current -= 20;
   }
if ((gamepad_button_check_pressed(0, gp_face2)) == true &amp;&amp; mp_current &gt;= 20){
    action_create_object_motion(spell2_obj, player1_obj.x+(xaxis*20), player1_obj.y+(yaxis*20), 45, dir1);
    mp_current -= 20;
   }
if ((gamepad_button_check_pressed(0, gp_face3)) == true &amp;&amp; mp_current &gt;= 100){
    action_create_object_motion(spell3_obj, player1_obj.x+(xaxis*20), player1_obj.y+(yaxis*20), 8, dir1);
    mp_current -= 100;
   }
if ((gamepad_button_check_pressed(0, gp_face4)) == true &amp;&amp; mp_current &gt;= 75){
        {
        var i;
            for (i = 0; i &lt; 10; i += 1)
             {
                action_create_object_motion(spell4_obj, player1_obj.x+((xaxis*20)+random_range(0, 150)), player1_obj.y+((yaxis*20)+random_range(0, 150)), 4, dir1);
             }
        }
    
    mp_current -= 75;
   }
// Keyboard controlls
if (keyboard_check(vk_left)) {
x -= 9;
}

if (keyboard_check(vk_right)) {
x += 9;
}

if (keyboard_check(vk_up)) {
player1_obj.y -= 9;
}

if (keyboard_check(vk_down)) {
player1_obj.y += 9;
}

if (keyboard_check_pressed(ord('A')) == true &amp;&amp; mp_current &gt;= 20){
    action_create_object_motion(spell1_obj, player1_obj.x+(hspeed*20), player1_obj.y+(vspeed*20), 35, dir2);
    mp_current -= 20;
   }
if (keyboard_check_pressed(ord('S')) == true &amp;&amp; mp_current &gt;= 20){
    action_create_object_motion(spell2_obj, player1_obj.x+(hspeed*20), player1_obj.y+(vspeed*20), 45, dir2);
    mp_current -= 20;
   }
if (keyboard_check_pressed(ord('D')) == true &amp;&amp; mp_current &gt;= 100){
    action_create_object_motion(spell3_obj, player1_obj.x+(hspeed*20), player1_obj.y+(vspeed*20), 8, dir2);
    mp_current -= 100;
   }
if (keyboard_check_pressed(ord('F')) == true &amp;&amp; mp_current &gt;= 75){
        {
        var i;
            for (i = 0; i &lt; 10; i += 1)
             {
                action_create_object_motion(spell4_obj, player1_obj.x+((hspeed*20)+random_range(0, 150)), player1_obj.y+((vspeed*20)+random_range(0, 150)), 4, dir2);
             }
        }
}
</string>
